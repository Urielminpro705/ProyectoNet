@page
@model Universidad.Pages_Inscripciones.DetailsModel

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles</h1>

<div>
    <h4>Inscripcion</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inscripcion.id_inscripcion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inscripcion.id_inscripcion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inscripcion.Curso)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inscripcion.Curso.titulo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inscripcion.Estudiante)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inscripcion.Estudiante.Nombre)
            @Html.DisplayFor(model => model.Inscripcion.Estudiante.Apellidos)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Inscripcion?.id_inscripcion">Editar</a> |
    <a asp-page="./Index">Volver a la lista</a>
</div>
